    "jquery": "^3.4.1",
    "node-sass": "^4.13.0",
    "popper.js": "^1.15.0",
    "react": "^16.9.0",
    "react-bootstrap": "^1.0.0-beta.16",
    "react-dom": "^16.9.0",
    "react-icons": "^3.8.0",
    "styled-components": "^5.0.0-beta.8"
  },
  "devDependencies": {
    "env-cmd": "^10.0.1",
    "prettier": "^1.18.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-hello-world"
